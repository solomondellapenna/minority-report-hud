  command = ${bsb_helper} ${bsb_helper_verbose} ${ocaml_dependencies} ${warnings} ${namespace} ${bs_super_errors} ${static_libraries} ${ocamlfind_dependencies} ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${in} -pack-native-library
  description = [34mBuilding[39m [2m${out}[22m
build lib.cmxa : build_cmxa_library  fake_src/sdl_index.cmx |  fake_src/sdl_index.cmi
build build.ninja : phony  ||  fake_src/sdl_index.mlast.d
